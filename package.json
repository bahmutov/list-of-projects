{
  "name": "list-of-projects",
  "description": "Personal NPM and GitHub projects web app",
  "main": "index.js",
  "dependencies": {
    "hydrate-app": "1.8.0",
    "lazy-ass": "1.6.0",
    "vue": "1.0.10-csp"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.25",
    "cross-env": "5.1.1",
    "css-loader": "^0.21.0",
    "cypress": "12.17.4",
    "cypress-await": "1.3.0",
    "cypress-map": "1.21.0",
    "git-issues": "1.3.1",
    "grunt": "0.4.5",
    "grunt-gh-pages": "1.0.0",
    "grunty": "0.2.0",
    "installcat": "0.2.0",
    "pre-git": "3.16.0",
    "prettier": "2.8.8",
    "standard": "10.0.3",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "vue-hot-reload-api": "^1.2.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "scripts": {
    "test": "npm run lint",
    "dev": "webpack-dev-server --inline --hot --quiet",
    "postinstall": "installcat",
    "issues": "git-issues",
    "build": "cross-env NODE_ENV=production webpack --progress --verbose",
    "lint": "standard --verbose --fix src webpack.config.js",
    "deploy": "npm run build && grunty grunt-gh-pages gh-pages gh-pages.json",
    "commit": "commit-wizard"
  },
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/list-of-projects.git"
  },
  "installcat": {
    "dist/hydrate-app.js": {
      "hydrate-app": "dist/hydrate-app.js"
    }
  },
  "keywords": [
    "web",
    "website",
    "personal",
    "projects"
  ],
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run lint",
        "npm test",
        "npm run build"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
